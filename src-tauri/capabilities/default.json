{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "binaries/goservices",
          "sidecar": true
        }
      ]
    },
    "shell:allow-open",
    "shell:allow-spawn"
  ]
}